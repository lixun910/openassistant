{
  "$schema": "https://typedoc.org/schema.json",
  "plugin": ["typedoc-plugin-markdown"],
  "entryPointStrategy": "packages",
  "entryPoints": [
    "../packages/tools/duckdb",
    "../packages/tools/geoda",
    "../packages/tools/h3",
    "../packages/tools/map",
    "../packages/tools/osm",
    "../packages/tools/places",
    "../packages/tools/plots",
    "../packages/components/assistant",
    "../packages/components/common",
    "../packages/components/echarts",
    "../packages/components/hooks",
    "../packages/components/keplergl",
    "../packages/components/leaflet",
    "../packages/components/tables",
    "../packages/components/vegalite",
    "../packages/utils"
  ],
  "out": "api",
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "skipErrorChecking": true,
  "githubPages": false,
  "hideGenerator": true,
  "includeVersion": true,
  "sort": ["kind", "alphabetical"],
  "kindSortOrder": [
    "Function",
    "Class",
    "Interface",
    "TypeAlias",
    "Variable"
  ]
}

