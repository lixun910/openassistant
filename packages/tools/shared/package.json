{
  "name": "@openassistant/tools-shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for OpenAssistant tools",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
